<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>firstApp</display-name>
	<welcome-file-list>

		<welcome-file>firstApp.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.org.loginform.loginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/loginServlet</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>login</filter-name>
		<filter-class>com.org.loginform.LoginFilterDemo</filter-class>

	</filter>
	<filter-mapping>
		<filter-name>login</filter-name>
		<url-pattern>/loginServlet</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>ResetPassword</servlet-name>
		<servlet-class>com.org.loginform.ResetPassword</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResetPassword</servlet-name>
		<url-pattern>/ResetPassword</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>soumya</servlet-name>
		<servlet-class>com.org.loginform.StudentRegistration1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>soumya</servlet-name>
		<url-pattern>/Priyanka</url-pattern>
	</servlet-mapping>



</web-app>